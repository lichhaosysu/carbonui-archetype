<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="carbonui">
	<fileSets>
		<fileSet filtered="true" packaged="true" encoding="UTF-8">
			<directory>src/main/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" packaged="true" encoding="UTF-8">
			<directory>src/main/resources/</directory>
			<includes>
				<include>**/*.properties</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" packaged="fasle" encoding="UTF-8">
			<directory>src/main/resources/</directory>
			<excludes>
				<exclude>**/*.properties</exclude>
			</excludes>
		</fileSet>
		<fileSet filtered="true" packaged="fasle" encoding="UTF-8">
			<directory>src/main/resources/web/folder_to_rename/js</directory>
		</fileSet>

		<fileSet filtered="true" packaged="true" encoding="UTF-8">
			<directory>src/test/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" packaged="false" encoding="UTF-8">
			<directory>src/test/resources</directory>
			<includes>
				<include>**/*.properties</include>
			</includes>
		</fileSet>
		<requiredProperties>
			<requiredProperty key="groupId">
				<defaultValue>com.gcidesign.paas</defaultValue>
			</requiredProperty>
			<requiredProperty key="version">
				<defaultValue>1.0.0-SNAPSHOT</defaultValue>
			</requiredProperty>
		</requiredProperties>
	</fileSets>
</archetype-descriptor>
